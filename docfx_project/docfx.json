{
	"$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
	"metadata": [
		{
			"src": [
				{
					"src": "..",
					"files": [
						"Albatross.Expression/Albatross.Expression.csproj"
					]
				}
			],
			"output": "api"
		}
	],
	"build": {
		"sitemap": {
			"baseUrl": "https://rushuiguan.github.io/expression",
			"priority": 0.5,
			"changefreq": "weekly"
		},
		"content": [
			{
				"files": [
					"**/*.{md,yml}"
				],
				"exclude": [
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"*.html",
					"robots.txt"
				]
			}
		],
		"output": "_site",
		"template": [
			"default",
			"modern"
		],
		"globalMetadata": {
			"_appName": "Albatross.Expression",
			"_appTitle": "Albatross.Expression",
			"_appLogoPath": "./images/logo.svg",
			"_appFaviconPath": "./images/favicon.svg",
			"_enableSearch": true,
			"pdf": false
		}
	}
}